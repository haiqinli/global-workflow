## aeroics with fcst reservation
rocotorun -w aero.xml -d aero.db
rocotostat -w aero.xml -d aero.db

## tracker only
rocotorun -w trak.xml -d trak.db
rocotostat  -w trak.xml -d trak.db

## archive only
rocotorun -w arch.xml -d arch.db
rocotostat -w arch.xml -d arch.db

## post, vfry, arch without reservation
rocotorun -w nores_ufscam_l64.xml -d nores_ufscam_l64.db
rocotostat  -w nores_ufscam_l64.xml -d nores_ufscam_l64.db

## fcst without reservation
rocotorun -w nores_l64_fcstonly.xml -d nores_l64_fcstonly.db
rocotostat -w nores_l64_fcstonly.xml -d nores_l64_fcstonly.db

## vrfy/arch w/ FCST reservation
rocotorun  -w restrak.xml  -d restrak.db

## aeroic w/ FCST reservation
rocotorun  -w resaeroic.xml  -d resaeroic.db

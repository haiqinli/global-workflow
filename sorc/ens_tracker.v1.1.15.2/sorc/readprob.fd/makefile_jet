SHELL=  /bin/sh
COMP=   ifort
FFLAGS= -O2 -check bounds -check format -fpe0 -axSSE4.2,AVX,CORE-AVX2

readprob:      readprob.f
	@echo " "
	@echo "  Compiling the TC genesis probability program....."
	$(COMP) $(FFLAGS) readprob.f -o readprob
	@echo " "

CMD = readprob

clean:
	-rm -f  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

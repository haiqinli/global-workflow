SHELL=  /bin/sh
COMP=   ifort
#FFLAGS= -O3 -g -traceback -convert big_endian
FFLAGS= -g -O3 -ftz -traceback -fpe0 -xHOST -axcore-avx512 -qno-opt-dynamic-align

readprob:      readprob.f
	@echo " "
	@echo "  Compiling the TC genesis probability program....."
	$(COMP) $(FFLAGS) readprob.f -o readprob
	@echo " "

CMD = readprob

clean:
	-rm -f  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

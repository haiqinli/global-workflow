SHELL=  /bin/sh
COMP=   ifort
FFLAGS= -O2 -check bounds -check format -fpe0 -axSSE4.2,AVX,CORE-AVX2

readprobLL:      readprobLL.f
	@echo " "
	@echo "  Compiling the TC genesis probability program....."
	$(COMP) $(FFLAGS) readprobLL.f -o readprobLL
	@echo " "

CMD = readprobLL

clean:
	-rm -f  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

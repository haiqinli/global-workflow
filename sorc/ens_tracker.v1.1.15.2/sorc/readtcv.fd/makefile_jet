SHELL=  /bin/sh
COMP=   ifort
FFLAGS= -O2 -check bounds -check format -fpe0 -axSSE4.2,AVX,CORE-AVX2

track_convert:      track_convert.f
	@echo " "
	@echo "  Compiling the TC track atcfunix to gen_format program....."
	$(COMP) $(FFLAGS) track_convert.f -o track_convert
	@echo " "

CMD =   track_convert

clean:
	-rm -f  *.o  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

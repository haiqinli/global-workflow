SHELL=  /bin/sh
COMP=   ifort
FFLAGS= -O3 -g -traceback -convert big_endian

leadtime:      leadtime.f
	@echo " "
	@echo "  Compiling the TC track leading hrs generation....."
	$(COMP) $(FFLAGS) leadtime.f -o leadtime
	@echo " "

CMD =   leadtime

clean:
	-rm -f  *.o  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)

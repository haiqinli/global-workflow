SHELL=  /bin/sh
COMP=   mpiifort
FFLAGS= -O3 -g -traceback -convert big_endian -axSSE4.2,AVX,CORE-AVX2

leadtime:      leadtime.f
	@echo " "
	@echo "  Compiling the TC track leading hrs generation....."
	$(COMP) $(FFLAGS) leadtime.f -o leadtime
	@echo " "

CMD =   leadtime

clean:
	-rm -f  *.o  *.mod

install:
	mv $(CMD) ../../exec/$(CMD)
